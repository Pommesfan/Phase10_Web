# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Controller for Sudoku

GET     /                           controllers.Phase10WebController.phase10

GET     /set_players                controllers.Phase10WebController.set_players(p1?="", p2?="", p3?="", p4?="")

GET     /about                      controllers.Phase10WebController.about

GET     /help                       controllers.Phase10WebController.help

GET     /game_move                  controllers.Phase10WebController.submitInput(game_move?="")

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
